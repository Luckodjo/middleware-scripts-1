pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                sleep 5
            }
        }
          stage('build') {
            steps {
                echo 'Hello built'
                sleep 5
            }
        }
          stage('test') {
            steps {
                echo 'Hello test'
                sleep 5
            }
        }
          stage('deploy') {
            steps {
                echo 'Hello deploy'
            }
        }
    }
}
